{
  "name": "ethicaltree-web",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applepicke/ethicaltree-web.git"
  },
  "scripts": {
    "deploy": "npm run build && npm run sync && npm run invalidate",
    "sync": "aws --profile ethicaltree s3 sync build/ s3://ethicaltree.com --delete",
    "invalidate": "aws --profile ethicaltree cloudfront create-invalidation --distribution-id ESEHHBYYH55J8 --paths \"/*\"",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path src",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --include-path src --watch --recursive -q",
    "start-js": "react-scripts start",
    "start": "PORT=3003 npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all -p build-css build-js",
    "simulate-prod": "env $(cat .env.development | xargs) npm run build && docker-compose up",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint ./src --quiet",
    "prettify": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@fnando/password_strength": "^1.1.4",
    "@types/googlemaps": "^3.30.5",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.0.36",
    "axios": "^0.16.1",
    "babel-plugin-ramda": "^1.4.3",
    "bootstrap": "^4.0.0",
    "bugsnag-js": "^4.7.2",
    "bugsnag-react": "^1.1.1",
    "classnames": "^2.2.5",
    "core-js": "^2.5.5",
    "js-file-download": "^0.4.1",
    "libphonenumber-js": "^1.6.7",
    "lodash": "^4.17.5",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.21",
    "noty": "^3.1.0",
    "npm-run-all": "^4.1.2",
    "numeral": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-autosuggest": "^9.4.0",
    "react-content-loader": "^3.2.0",
    "react-dom": "^16.1.1",
    "react-facebook-pixel": "^0.1.1",
    "react-ga": "^2.2.0",
    "react-google-maps": "^9.4.5",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-modal": "^3.1.3",
    "react-paginate": "^5.0.0",
    "react-phone-number-input": "^1.1.12",
    "react-redux": "^5.0.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-s3-uploader": "^4.5.1",
    "react-scripts": "^1.1.5",
    "react-select": "^1.2.1",
    "react-slick": "^0.17.1",
    "react-spinkit": "^3.0.0",
    "react-toggle": "^4.0.2",
    "react-transition-group": "^2.2.1",
    "react-wufoo-embed": "^1.0.0",
    "reactstrap": "^5.0.0-alpha.4",
