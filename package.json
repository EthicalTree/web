{
  "name": "ethicaltree-web",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applepicke/ethicaltree-web.git"
  },
  "scripts": {
    "deploy": "npm run build && npm run sync && npm run invalidate",
    "sync": "aws --profile ethicaltree s3 sync build/ s3://beta.ethicaltree.com --delete",
    "invalidate": "aws --profile ethicaltree cloudfront create-invalidation --distribution-id ESEHHBYYH55J8 --paths \"/*\"",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path src",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --include-path src --watch --recursive -q",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
